<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profile"
    android:background="@drawable/iw8">


<TextView
        android:id="@+id/greeting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="User Info"
        android:textStyle="italic"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="15dp"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        />


    <TextView
        android:id="@+id/emailAddressTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email Address:"
        android:textStyle="bold"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        app:layout_constraintTop_toTopOf="@+id/greeting"
        app:layout_constraintLeft_toLeftOf="@+id/greeting"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="80dp"
        />

    <TextView
        android:id="@+id/emailAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/emailAddressTitle"
        app:layout_constraintLeft_toLeftOf="@+id/emailAddressTitle"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="30dp"
        android:textSize="12sp"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        />

    <TextView
        android:id="@+id/fullNameTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Operator Name:"
        android:textStyle="bold"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        app:layout_constraintTop_toTopOf="@+id/emailAddress"
        app:layout_constraintLeft_toLeftOf="@+id/greeting"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="80dp"
        />

    <TextView
        android:id="@+id/fullName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/fullNameTitle"
        app:layout_constraintLeft_toLeftOf="@+id/emailAddressTitle"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="30dp"
        android:textSize="12sp"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        />

    <TextView
        android:id="@+id/plantTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Plant/Site name:"
        android:textStyle="bold"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        app:layout_constraintTop_toTopOf="@+id/fullName"
        app:layout_constraintLeft_toLeftOf="@+id/fullNameTitle"
        android:layout_marginTop="15dp"
        />

    <TextView
        android:id="@+id/plant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/plantTitle"
        app:layout_constraintLeft_toLeftOf="@+id/fullNameTitle"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="30dp"
        android:textSize="12sp"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        />

    <TextView
        android:id="@+id/greeting2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Real-Time Plant Data"
        android:textStyle="italic"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        app:layout_constraintTop_toTopOf="@id/plant"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="15dp"
        />

    <TextView
        android:id="@+id/pHTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="pH (TAG-PH001):"
        android:textStyle="bold"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        app:layout_constraintTop_toTopOf="@+id/greeting2"
        app:layout_constraintLeft_toLeftOf="@+id/greeting2"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        />

    <TextView
        android:id="@+id/pHid_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/pHTitle"
        app:layout_constraintLeft_toLeftOf="@+id/pHTitle"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="30dp"
        android:textSize="12sp"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        />

    <TextView
        android:id="@+id/tempTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Temperature (TAG-TP001):"
        android:textStyle="bold"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        app:layout_constraintTop_toTopOf="@+id/pHid_"
        app:layout_constraintLeft_toLeftOf="@+id/greeting2"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="20dp"
        />
    <TextView
        android:id="@+id/Tempid_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/tempTitle"
        app:layout_constraintLeft_toLeftOf="@+id/tempTitle"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="30dp"
        android:textSize="12sp"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        />

    <TextView
        android:id="@+id/turbTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Turbidity (TAG-TU001):"
        android:textStyle="bold"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        app:layout_constraintTop_toTopOf="@+id/Tempid_"
        app:layout_constraintLeft_toLeftOf="@+id/greeting2"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="20dp"
        />
    <TextView
        android:id="@+id/TU001id_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/turbTitle"
        app:layout_constraintLeft_toLeftOf="@+id/turbTitle"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="30dp"
        android:textSize="12sp"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        />

    <TextView
        android:id="@+id/FL001Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="In - Flow (TAG-FL001):"
        android:textStyle="bold"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        app:layout_constraintTop_toTopOf="@+id/TU001id_"
        app:layout_constraintLeft_toLeftOf="@+id/greeting2"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="20dp"
        />
    <TextView
        android:id="@+id/FL001id_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/FL001Title"
        app:layout_constraintLeft_toLeftOf="@+id/FL001Title"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="30dp"
        android:textSize="12sp"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        />

    <TextView
        android:id="@+id/CL001Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chlorine Residual (TAG-CL001):"
        android:textStyle="bold"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        app:layout_constraintTop_toTopOf="@+id/FL001id_"
        app:layout_constraintLeft_toLeftOf="@+id/greeting2"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="20dp"
        />
    <TextView
        android:id="@+id/CL001id_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/CL001Title"
        app:layout_constraintLeft_toLeftOf="@+id/CL001Title"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="30dp"
        android:textSize="12sp"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        />
    <TextView
        android:id="@+id/LI001Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reservoir Level (TAG-LI001):"
        android:textStyle="bold"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        app:layout_constraintTop_toTopOf="@+id/CL001id_"
        app:layout_constraintLeft_toLeftOf="@+id/greeting2"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="20dp"
        />
    <TextView
        android:id="@+id/LI001id_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/LI001Title"
        app:layout_constraintLeft_toLeftOf="@+id/LI001Title"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="30dp"
        android:textSize="12sp"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        />

    <TextView
        android:id="@+id/CL002Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chlorine Validation (TAG-CL002):"
        android:textStyle="bold"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        app:layout_constraintTop_toTopOf="@+id/LI001id_"
        app:layout_constraintLeft_toLeftOf="@+id/greeting2"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="20dp"
        />
    <TextView
        android:id="@+id/CL002id_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/CL002Title"
        app:layout_constraintLeft_toLeftOf="@+id/CL002Title"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="30dp"
        android:textSize="12sp"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        />

    <TextView
        android:id="@+id/FL004Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Out - Flow (TAG-FL004):"
        android:textStyle="bold"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        app:layout_constraintTop_toTopOf="@+id/CL002id_"
        app:layout_constraintLeft_toLeftOf="@+id/greeting2"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="20dp"
        />
    <TextView
        android:id="@+id/FL004id_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/FL004Title"
        app:layout_constraintLeft_toLeftOf="@+id/FL004Title"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="30dp"
        android:textSize="12sp"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        />

    <TextView
        android:id="@+id/MLTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AI Suggested SP Value (TAG-SP23):"
        android:textStyle="bold"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        app:layout_constraintTop_toTopOf="@+id/FL004id_"
        app:layout_constraintLeft_toLeftOf="@+id/greeting2"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="20dp"
        />
    <TextView
        android:id="@+id/MLid_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/MLTitle"
        app:layout_constraintLeft_toLeftOf="@+id/MLTitle"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="30dp"
        android:textSize="12sp"
        android:textColor="@color/material_on_primary_emphasis_high_type"
        />

    <Button
        android:id="@+id/logOut"
        android:layout_width="200dp"
        android:layout_height="45dp"
        android:layout_marginTop="60dp"
        android:text="Logout"
        android:textColor="#ffffff"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/FL004id_"
        />







</androidx.constraintlayout.widget.ConstraintLayout>